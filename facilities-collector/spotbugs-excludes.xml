<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- Exclusions can be defined here and should include a comment on why the finding can be ignored -->
  <!-- Lombok lazy getters -->
  <Match>
    <Or>
      <Method name="equals"/>
      <Method name="hashCode"/>
    </Or>
    <Bug pattern="RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE"/>
  </Match>
</FindBugsFilter>